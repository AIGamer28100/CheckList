# Development Environment Configuration Template
# Copy this file to .env.dev and fill in your development values

# =============================================
# FIREBASE CONFIGURATION (DEVELOPMENT)
# =============================================
FIREBASE_API_KEY=your_dev_firebase_api_key_here
FIREBASE_AUTH_DOMAIN=your_dev_project_id.firebaseapp.com
FIREBASE_PROJECT_ID=your_dev_project_id_here
FIREBASE_STORAGE_BUCKET=your_dev_project_id.firebasestorage.app
FIREBASE_MESSAGING_SENDER_ID=your_dev_sender_id_here
FIREBASE_APP_ID=1:your_dev_sender_id:web:your_dev_app_id_here

# =============================================
# DEVELOPMENT-SPECIFIC SETTINGS
# =============================================
APP_ENV=development
DEBUG_MODE=true
API_TIMEOUT=30000
ENABLE_LOGGING=true

# Authentication Providers (Dev)
GOOGLE_CLIENT_ID_WEB=your_dev_google_web_client_id.apps.googleusercontent.com
GITHUB_CLIENT_ID=your_dev_github_client_id_here
GITHUB_CLIENT_SECRET=your_dev_github_client_secret_here