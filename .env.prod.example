# Production Environment Template
# Copy this file to .env.prod and fill in your actual production values

# Firebase Configuration (Production)
FIREBASE_API_KEY=your_prod_firebase_api_key_here
FIREBASE_AUTH_DOMAIN=your_prod_project.firebaseapp.com
FIREBASE_PROJECT_ID=your_prod_project_id_here
FIREBASE_STORAGE_BUCKET=your_prod_project.appspot.com
FIREBASE_MESSAGING_SENDER_ID=987654321
FIREBASE_APP_ID=1:987654321:web:fedcba654321

# Other API Keys (Production)
# GOOGLE_MAPS_API_KEY=your_prod_google_maps_key
# STRIPE_PUBLISHABLE_KEY=your_prod_stripe_key

# Environment Settings
APP_ENV=production
DEBUG_MODE=false