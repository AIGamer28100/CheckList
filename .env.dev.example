# Development Environment Template
# Copy this file to .env.dev and fill in your actual development values

# Firebase Configuration (Development)
FIREBASE_API_KEY=your_dev_firebase_api_key_here
FIREBASE_AUTH_DOMAIN=your_dev_project.firebaseapp.com
FIREBASE_PROJECT_ID=your_dev_project_id_here
FIREBASE_STORAGE_BUCKET=your_dev_project.appspot.com
FIREBASE_MESSAGING_SENDER_ID=123456789
FIREBASE_APP_ID=1:123456789:web:abcdef123456

# Other API Keys (Development)
# GOOGLE_MAPS_API_KEY=your_dev_google_maps_key
# STRIPE_PUBLISHABLE_KEY=your_dev_stripe_key

# Environment Settings
APP_ENV=development
DEBUG_MODE=true