# Environment Configuration
# Copy this file to .env and fill in your actual values

# Firebase/Google Cloud Configuration
FIREBASE_PROJECT_ID=your-project-id
FIREBASE_API_KEY=your-api-key
FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
FIREBASE_STORAGE_BUCKET=your-project.firebasestorage.app
FIREBASE_MESSAGING_SENDER_ID=your-sender-id
FIREBASE_APP_ID=your-app-id
GOOGLE_CLOUD_PROJECT=your-project-id

# Authentication Provider Configuration
# Google Sign-In Client ID (Web)
GOOGLE_CLIENT_ID_WEB=your-web-client-id.apps.googleusercontent.com
# Google Sign-In Client ID (iOS) 
GOOGLE_CLIENT_ID_IOS=your-ios-client-id.apps.googleusercontent.com
# Google Sign-In Client ID (Android)
GOOGLE_CLIENT_ID_ANDROID=your-android-client-id.apps.googleusercontent.com

# GitHub OAuth App Configuration
GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret
# GitHub Personal Access Token (for GitHub Integration)
GITHUB_TOKEN=ghp_your_personal_access_token

# API Server Configuration (Optional - for backend API)
API_SERVER_URL=http://localhost:8000
API_SERVER_PORT=8000

# Other API Keys (Optional)
# GOOGLE_MAPS_API_KEY=your-google-maps-key
# STRIPE_PUBLISHABLE_KEY=your-stripe-key
# ANALYTICS_KEY=your-analytics-key

# Database URLs (Optional)
# DATABASE_URL=your-database-url

# Environment (development, staging, production)
APP_ENV=development
DEBUG_MODE=true
