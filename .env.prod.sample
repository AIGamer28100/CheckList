# Production Environment Configuration Template
# Copy this file to .env.prod and fill in your production values

# =============================================
# FIREBASE CONFIGURATION (PRODUCTION)
# =============================================
FIREBASE_API_KEY=your_prod_firebase_api_key_here
FIREBASE_AUTH_DOMAIN=your_prod_project_id.firebaseapp.com
FIREBASE_PROJECT_ID=your_prod_project_id_here
FIREBASE_STORAGE_BUCKET=your_prod_project_id.firebasestorage.app
FIREBASE_MESSAGING_SENDER_ID=your_prod_sender_id_here
FIREBASE_APP_ID=1:your_prod_sender_id:web:your_prod_app_id_here

# =============================================
# PRODUCTION-SPECIFIC SETTINGS
# =============================================
APP_ENV=production
DEBUG_MODE=false
API_TIMEOUT=15000
ENABLE_LOGGING=false

# Authentication Providers (Prod)
GOOGLE_CLIENT_ID_WEB=your_prod_google_web_client_id.apps.googleusercontent.com
GITHUB_CLIENT_ID=your_prod_github_client_id_here
GITHUB_CLIENT_SECRET=your_prod_github_client_secret_here

# Production Analytics
GOOGLE_ANALYTICS_TRACKING_ID=UA-your_prod_tracking_id
FIREBASE_MEASUREMENT_ID=G-your_prod_measurement_id